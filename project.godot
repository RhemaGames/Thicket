; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Thicket"
run/main_scene="res://elements/clouds.tscn"
config/features=PackedStringArray("4.3")
boot_splash/bg_color=Color(0.00351917, 0.00351917, 0.00351916, 1)
boot_splash/image="res://icon.png"
boot_splash/fullsize=false
config/icon="res://icon.png"

[autoload]

Thicket="res://elements/Thicket.tscn"
OpenSeed="res://OpenSeed-Godot/OpenSeed.tscn"

[display]

window/size/viewport_width=1900
window/size/viewport_height=1060
window/stretch/mode="2d"
window/stretch/aspect="expand"
window/size/fullscreen=true

[editor_plugins]

enabled=PackedStringArray("res://addons/thicket_sdk/plugin.cfg")

[gui]

theme/use_hidpi=true

[memory]

limits/message_queue/max_size_kb=2048

[rendering]

environment/defaults/default_environment="res://default_env.tres"
quality/driver/fallback_to_gles2=true
threads/thread_model=2
vram_compression/import_etc=true
